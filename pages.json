				"navigationBarTitleText": "icon案例"
			}
		},
		{
			"path":"pages/test/basics/text",
			"style":{
				"navigationBarTitleText": "text案例"
			}
		},
		{
			"path":"pages/test/basics/rich-text",
			"style":{
				"navigationBarTitleText": "rich-text案例"
			}
		},
		{
			"path":"pages/test/basics/progress",
			"style":{
				"navigationBarTitleText": "progress案例"
			}
		},
		{
			"path":"pages/test/form/button",
