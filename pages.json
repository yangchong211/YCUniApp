		{
			"path":"pages/test/form/form",
			"style":{
				"navigationBarTitleText": "form案例"
			}
		},
		{
			"path":"pages/study/qq-home",
			"style":{
				"navigationBarTitleText": "form案例"
			}
		},
		{
			"path":"pages/study/qq-serach",
			"style":{
				"navigationBarTitleText": "form案例"
			}
		},
		{
			"path":"pages/study/qq-mine",
			"style":{
				"navigationBarTitleText": "form案例"
			}
		}
