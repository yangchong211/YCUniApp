{
	// 设置页面路径及窗口表现，必须的
	// uni-app 通过 pages 节点配置应用由哪些页面组成，pages 节点接收一个数组，数组每个项都是一个对象
	"pages": [
		//应用中新增/减少页面，都需要对 pages 数组进行修改
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		// {
		// 	// 配置页面路径
		// 	"path": "pages/index/index",
		// 	// 配置页面窗口表现
		// 	// 用于设置每个页面的状态栏、导航条、标题、窗口背景色等。
		// 	"style": {
		// 		"navigationBarTitleText": "uni-app-yc"
		// 	}
		// },
		{
			"path": "pages/tab/home",
			"style": {
				"navigationBarTitleText": "首页",
				//是否开启下拉刷新
				"enablePullDownRefresh": false,
				//配置编译到 App 平台时的特定样式，部分常用配置 H5 平台也支持。
				"app-plus": {
					//禁用原生导航栏
					"titleNView": true,
					//原生标题栏按钮配置,
					"buttons": [{
						//原生标题栏增加分享按钮，点击事件可通过页面的 onNavigationBarButtonTap 函数进行监听
						"text": "分享"
					}],
					//自定义 backButton
					"backButton": {
						"background": "#00FF00"
					},
					//下拉刷新
					"pullToRefresh": {
						"support": true,
						"color": "#ff3333",
						"style": "default",
						"contentdown": {
							"caption": "下拉可刷新自定义文本"
						},
						"contentover": {
							"caption": "释放可刷新自定义文本"
						},
						"contentrefresh": {
							"caption": "正在刷新自定义文本"
						}
					}
				}
			}
		},
		{
			"path": "pages/tab/classify",
			"style": {
				"navigationBarTitleText": "分类"
			}
		},
		{
			"path": "pages/tab/study",
			"style": {
				"navigationBarTitleText": "学习"
			}
		},
		{
			"path": "pages/tab/mine",
			"style": {
				"navigationBarTitleText": "我的"
			}
		},
		{
			"path": "pages/web/web",
			"style": {
				"navigationBarTitleText": "详情",
				"app-plus": {
					"titleNView": {
						//透明渐变导航栏 App-nvue 2.4.4+ 支持
						"type": "transparent"
					}
				}
			}
		},
		{
			"path": "pages/search/search",
			"style": {
				"navigationBarTitleText": "搜索",
				"app-plus": {
					"titleNView": {
						//透明渐变导航栏 App-nvue 2.4.4+ 支持
						"type": "transparent",
						"searchInput": {
							"backgroundColor": "#fff",
							//输入框圆角
							"borderRadius": "6px",
							"placeholder": "请输入搜索内容",
							//disable时点击输入框不置焦，可以跳到新页面搜索
							"disabled": true
						}
					}
				}
			}
		},
		{
			"path": "pages/mine/setting",
			"style": {
				"navigationBarTitleText": "设置中心",
				"backgroundColor":"#FFFFFF",
				"navigationBarBackgroundColor":"#FFFFFF"
			}
		},
		{
			"path": "pages/android/android",
			"style": {
				"navigationBarTitleText": "玩Android",
				"app-plus": {
					//禁用原生导航栏
					"titleNView": false,
					//侧滑菜单
					"subNVues": [{
						//subNVue 的 id，可通过 uni.getSubNVueById('drawer') 获取
						"id": "drawer",
						// nvue 路径
						"path": "pages/index/drawer.nvue",
						"style": { //webview style 子集，文档可暂时开放出来位置，大小相关配置
							//除 popup 外，其他值域参考 5+ webview position 文档
							"position": "popup",
							"width": "50%"
						}

					}, { //弹出层
						"id": "popup",
						"path": "pages/index/popup",
						"style": {
							"position": "popup",
							"margin": "auto",
							"width": "150px",
							"height": "150px"
						}

					}, { //自定义头
						"id": "nav",
						"path": "pages/index/nav",
						"style": {
							"position": "dock",
							"height": "44px"
						}
					}]
				}
			}
		},
		{
			"path":"pages/test/test1",
			"style":{
				"navigationBarTitleText": "测试"
			}
			
		},
